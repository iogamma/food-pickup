
    <div class="row custOrders">
      <div class="col-md-12">
        <div class= "row orderInfo">
          <div class= "col-md-3 orderId">
            Order ID:
            <% for (objec in myOrders) { %>
              <%= objec %>
            <% } %>
          <div class= "col-md-4 custName">
            Customer name:
            <% for (usr in myUsers) { %>
              <%= usr %>
            <% } %>
          </div>
          <div class= "col-md-5 phoneNumber">
            Phone Number:
            <% for (numb in myNumbers) { %>
              <%= numb %>
            <% } %>
          </div>
        </div>
        <% theOrder.forEach(function(element, index) { %>
        <div class= "row foodItem">
          <div class= "col-md-10 itemName">
            Item:
            <%= theOrder[index].name %>
          </div>
          <div class= "col-md-2 quantity">
            Quantity:
            <%= theOrder[index].quantity %>
          </div>
        </div>
        <div class= "row responseToOrder">
          <div class= "text-center col-md-6">
            Please inform customer of order pick up time:
          </div>
          <form action="/restaurants/1/pickup" method="POST">
            <div class= "text-right col-md-3">
              <input type="number" class="form-control" id="ready_time" name="ready_time" placeholder="minutes"/>
              <input type="hidden" class="form-control" id="order_id" name="order_id" value=<%= theOrder[index].order_id %> />
            </div>
            <div class= "text-center col-md-3">
              <input type="submit" class="btn btn-primary"></input>
            </div>
          </form>
        </div>
        <% }); %>

          </div>
        </div>
      </div>
    </div>
